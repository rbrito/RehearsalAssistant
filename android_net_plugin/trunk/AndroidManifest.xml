<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="urbanstew.RehearsalAssistant.NetPlugin"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".RehearsalAssistantNetPlugin"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".NetBroadcastListener" android:label="Net Plugin">
	    <intent-filter>
	        <action android:name="urbanstew.RehearsalAssistant.NetPlugin.startSession" />
	    </intent-filter>
	    <intent-filter>
	        <action android:name="urbanstew.RehearsalAssistant.NetPlugin.stopSession" />
	    </intent-filter>
	    <intent-filter>
	        <action android:name="urbanstew.RehearsalAssistant.NetPlugin.playbackStarted" />
	    </intent-filter>
	    </receiver>
        
    </application>
    <uses-sdk android:minSdkVersion="2" />

<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest> 