<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      android:versionCode="1"
      package="urbanstew.RehearsalAssistant" android:versionName="0.0.1">
<application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="true">
    <activity android:label="@string/app_name" android:name=".RehearsalAssistant">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
    <activity android:name="RehearsalPlayback"><intent-filter android:label="android.intent.action.VIEW"></intent-filter>
	</activity>
	<activity android:name="RehearsalRecord"><intent-filter android:label="android.intent.action.EDIT"></intent-filter>
</activity>
</application>

<uses-permission android:name="android.permission.VIBRATE"></uses-permission>

<uses-permission android:name="android.permission.RECORD_AUDIO"></uses-permission>

</manifest> 